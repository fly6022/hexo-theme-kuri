<!DOCTYPE html>
<html lang="<%= config.language %>">
 <head> 
  <meta charset="utf-8" /> 
  <title><%= config.title %></title> 
  <link rel="icon" href="<%= theme.fav %>" /> 
  <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
  <meta name="keywords" content="<%= config.keywords %>" />
  <meta name="description" content="<%= config.description %>" /> 
  <link rel="canonical" href="<%= config.url %>" /> 
  <link rel="stylesheet" href="/css/style.css" /> 
  <link rel="stylesheet" href="/css/mdui.min.css" importance="high"/> 
  <% if (theme.aplayer.enable) { %>
  <link rel="stylesheet" href="https://blog.kuri.ink/lib/aplayer@1.10.0/aplayer.min.css" /> 
  <% } %>
  <script src="/js/mdui.min.js" importance="high"></script>   
 </head> 
 <body> 
   <%- include("_widget/cover.ejs") %>
  <div class="mdui-drawer-body-left mdui-appbar-with-toolbar mdui-theme-layout-auto mdui-loaded"> 
   <!-- background --> 
   <div id="bg" class="index_bg"></div> 
   <!-- main --> 
   <div>
    
    <%- include("_partial/header.ejs") %> 

    <div class="mdui-drawer mdui-drawer-close sidebar" id="main-drawer"> 
     <div class="mdui-list" mdui-collapse="{accordion: true}" style="margin-bottom: 75px;"> 
      <% for (c in theme.sidebar) { %> 
      <div class="
                mdui-collapse-item
                "> 
       <div class="mdui-collapse-item-header mdui-list-item mdui-ripple"> 
        <i class="
                    mdui-list-item-icon
                    mdui-icon
                    material-icons
                    <%= theme.sidebar_color[c][1] %>"><%= theme.sidebar_color[c][0] %></i>
        <div class="mdui-list-item-content">
         <%= c %>
        </div> 
        <i class="mdui-collapse-item-arrow mdui-icon material-icons">keyboard_arrow_down</i> 
       </div>
       <% for (d in theme.sidebar[c]) { %> 
         <div class="mdui-collapse-item-body mdui-list"> 
          <a href="<%= theme.sidebar[c][d][0] %>" target="_blank" mdui-tooltip="{content: '<%= theme.sidebar[c][d][1] %>'}" class="
                        mdui-list-item
                        mdui-ripple
                        "><%= d %></a> 
         </div>
         <% } %>
      </div> 
      <% } %>
     </div> 
    </div> 
   </div> 

   <%- include("_partial/container.ejs") %> 

   <!-- footer --> 
   <div id="footer" class="footer"> 
    <p class="remark">
      <%- theme.remark %><br>
      <%- theme.regist %>
      <%- include("_partial/LICENSE.EJS") %>
    </p> 
   </div>  
   <script async="" src="https://blog.kuri.ink/lib/aplayer@1.10.0/aplayer.min.js" type="text/javascript"></script> 
   <script async="" src="https://blog.kuri.ink/lib/meting/meting.min.js" type="text/javascript"></script>

   <% if (theme.aplayer.enable) { %>
      <meting-js style="position: absolute; z-index:99999;" type="<%= theme.aplayer.type %>" server="<%= theme.aplayer.server %>" id="<%= theme.aplayer.id %>" fixed="<%= theme.aplayer.fixed %>"></meting-js> 
   <% } %>

   <% if (theme.analysis.platform == "51.la") { %>
      <script charset="UTF-8" id="LA_COLLECT" src="//sdk.51.la/js-sdk-pro.min.js"></script>
      <script>LA.init({id: "<%= theme.analysis.id %>",ck: "<%= theme.analysis.cd %>"})</script>
   <% } %>

  </div>
 </body>
</html>